root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.c]
indent_size = 2

[{compam,neckam,partam}.c]
indent_size = unset

[{LICENSE,emacs.el}]
indent_size = unset

[Makefile]
indent_style = tab

[VERSION]
insert_final_newline = false
